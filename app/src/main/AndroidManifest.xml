<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eudev.bloodbank.bloodbankeu">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <permission android:name="android.permission.CALL_PRIVILEGED" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.register.RegisterActivity" />
        <activity
            android:name=".activity.activity.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.activity.UserDetailsActivity" />
        <activity android:name=".activity.activity.BloodRequestActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.activity.BloodRequestActivity" />
        </activity>
        <activity android:name=".activity.activity.SearchActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.activity.SearchActivity" />
        </activity>
        >
        <activity android:name=".activity.activity.AddNewMemberActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.activity.AddNewMemberActivity" />
        </activity>
        <activity android:name=".activity.activity.EditProfileActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.activity.EditProfileActivity" />
        </activity>
        <activity android:name=".activity.activity.DeveloperActivity"></activity>
    </application>

</manifest>